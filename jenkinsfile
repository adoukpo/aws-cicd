pipeline {
    agent any
    stages {
    stage('git checkout'){
        steps{
            git branch: 'main', url: 'https://github.com/adoukpo/aws-cicd.git'
        }
    }
    stage('ls'){
        steps{
            sh 'echo ls'
        }
    }
    stage('test'){
        steps{
            sh 'echo test'
        }
    }
    stage('pwd'){
        steps{
            sh 'echo pwd'
        }
    }
    stage('nproc'){
        steps{
            sh 'echo nproc'
        }
    }
    stage('whoami'){
        stages{
            sh 'echo whoami'
        }
    }
    }
}